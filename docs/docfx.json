{
  "metadata": [
    {
      "src": [
        {
          "files": [ "SafeApp/*.csproj", "SafeApp.Core/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "toc.yml", "*.md", "api/index.md" ]
      },
      {
        "files": [ "CHANGELOG.md" ],
        "src": "../"
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": [
      {
        "files": [ "apidoc/**.md" ],
        "exclude": [ "obj/**", "_site/**" ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "theme/src"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Safe Network :: safe_app_csharp API Documentation",
      "_appLogoPath": "./images/logo.svg",
      "_appFaviconPath": "./images/favicon.ico"
    }
  }
}